                                        str = str + "<tr style= \"border-bottom: 1px solid #99999938;font-weight: bold;\"><td  colspan=\"1\"> Dịch vụ: <a style= \"color:red;\">" + lst[0].DICH_VU +
                                            "</a></td><td  colspan=\"1\">Nơi đến: <a style= \"color:red;\">" + lst[0].NOI_DEN + "</a></td>" +
                                            "</td><td  colspan=\"1\">Trọng lượng: <a style= \"color:red;\">" + lst[0].TRONG_LUONG + " kg</a></td>"
                                            + "</tr> ";
                                    }
                                    for (var j in lst) {
                                        str = str + "<tr style= \"border-bottom: 1px solid #99999938;\"><td colspan=\"1\">" + lst[j].INSERT_TIME_STRING + "</td><td colspan=\"2\">" + lst[j].DESC + "</td></tr>";
                                    }
                                }
                                $('#listTrackAndTrace tbody').append(str);
                                $('#mess_vandon').hide();
                                $('.img-loading').hide();
                                $('.listTrackAndTrace').show();
                            }


                            else {
                                $('#mess_vandon').html("@Html.Raw(Resource.khongtimthayvandon)" + "<a class='timtrangcu' onclick='TrangCu()'> Mời bạn ấn vào đây để tìm trên trang web cũ</a>");
                                $('#mess_vandon').show();
                                $('.listTrackAndTrace').hide();
                            }
                        }
                    });
            }

                $('#trackandtrade').show();
                $('#billnoidia').val('');
                $('.dr-tracuu').fadeOut();
                if ($('#banner').length > 0) {
                    if (screen.width > 1024) {
                        $('html,body').animate({ scrollTop: 500 }, 300);
                    }
                }
        }

        function ShowAnhLienVang(idmongo, URLImage, idAnhPOD) {
            $("#viewer").hide();
            $('#idAnhPOD').val(idAnhPOD);
            $.ajax({
                url: "/Home/GetImageFromMongoDB",
                type: "POST",
                data: {
                    idmongo: idmongo,
                    UrlImage: URLImage
                },
                dataType: 'json',
                success: function (data) {
                    iv1.iviewer('loadImage', data.Title);
                    setTimeout(function () {
                        iv1.iviewer('set_zoom', 100);
                    },5);
                    $('.pre-image').hide();
                    $("#viewer").show();
                }
            });
            if ((idmongo == null || idmongo == "") && (URLImage.indexOf(".tif") == -1)) {
                $('#modal-anhlienvang .modal-footer').show();
            }
            else {
                $('#modal-anhlienvang .modal-footer').hide();
            }
            $('.pre-image').show();
            $('#modal-anhlienvang').modal('show');

        }

        function baocaosaipham() {
            if ($('#txtNote').val() == undefined || $('#txtNote').val() == "") {
                Swal.fire('@Html.Raw(Resource.vadnoidung)',
                    '',
                    'error')
                return;
            }
            $.ajax({
                url: "/Home/BaoCaoSaiPham",
                type: "POST",
                data: {
                    sdt:$('#txtsdtlienhe').val(),
                    note: $('#txtNote').val(),
                    idAnhPOD: $('#idAnhPOD').val()
                },
                dataType: 'json',
                success: function (data) {
                    if (!data.Error) {
                        Swal.fire('@Html.Raw(Resource.baocaosaiphamthanhcong)',
                            '',
                            'success');
                        $('#modal-baocaosaipham').modal('hide');
                        $('#txtNote').val('');
                    }
                    else {
                        Swal.fire(data.Title,
                            '',
                            'error')
                        return;
                    }
                }
            });
        }

        $('#modal-baocaosaipham').on('shown.bs.modal', function () {
            $('#txtsdtlienhe').trigger('focus')
        })

