            function registerClientMethods(chatHub) {
                chatHub.client.checkBranchOnline = function (isCSMB, isCSMN) {
                    if (isCSMB || isCSMN) {
                        $('#cs-online').removeClass("d-none");
                        $('#cs-offline').addClass("d-none");
                    }
                    else {
                        $('#cs-online').addClass("d-none");
                        $('#cs-offline').removeClass("d-none");
                    }

                    if (isCSMB) {
                        $('#HNbranch').html('<span style="color: green;"> online</span>');
                    }
                    else {
                        $('#HNbranch').html('<span> offline</span>');
                    }
                    if (isCSMN) {
                        $('#DNbranch').html('<span style="color: green;"> online</span>');
                        $('#HCMbranch').html('<span style="color: green;"> online</span>');
                    }
                    else {
                        $('#DNbranch').html('<span> offline</span>');
                        $('#HCMbranch').html('<span> offline</span>');
                    }
                }

                chatHub.client.onConnected = function (UserID, data, ChiNhanh) {
                    $('.chat-mess-title p strong').html(ChiNhanh);
                    $('.chat-message-content').empty();
                    for (var i in data) {
                        if (data[i].Fr_UserID !== UserID) {
                            $('.chat-message-content').append('<div class="col-md-12"><div class="chat-message-text pull-left"><p><strong>' + data[i].Fr_UserName +'</strong><br/> ' + data[i].Message + '</p><p class="time-log"><small>'+data[i].Time_String+'</small></p></div></div>');
                        }
                        else {
                            $('.chat-message-content').append('<div class="col-md-12"><div class="chat-message-text pull-right"><p> ' + data[i].Message + ' <p> </div></div>');
                        }
                    }
                    $('.chat-message-loading').addClass(" d-none");
                }

                chatHub.client.sendPrivateMessage = function (dtoMess, UserNameSend, NameSend) {
                    if (dtoMess.Fr_UserRole == "CS" || dtoMess.Fr_UserRole == "CS_ADMIN") {
                        ua = navigator.userAgent;
                        /* MSIE used to detect old browsers and Trident used to newer ones*/
                        var is_ie = ua.indexOf("MSIE ") > -1 || ua.indexOf("Trident/") > -1;

                        if (is_ie == false) {
                            //notification
                            if (window.Notification && Notification.permission === "granted") {
                                var notification = new Notification(dtoMess.Fr_UserName, {
                                    icon: "/lib/images/logo/logo.png",
                                    body: dtoMess.Message,
                                });

                                notification.onclick = function () {
                                    window.focus();
                                    this.close();
                                };
                                setTimeout(function () {
                                    notification.close();
                                }, 5000);
                            } else {
                                Notification.requestPermission();
                            }
                        }

                        $('.chat-message-content').append('<div class="col-md-12"><div class="chat-message-text pull-left"><p><strong>' + dtoMess.Fr_UserName + '</strong><br/>' + dtoMess.Message + '</p><p class="time-log"><small>'+dtoMess.Time_String+'</small></p></div></div>');
                        var height = $('.chat-message-content')[0].scrollHeight;
                        $('.chat-message-content').scrollTop(height);
                        if (!$('#live-chat').hasClass("d-none")) {
                            chatHub.server.updateReadMess(dtoMess.To_UserID, "KHACH");
                        }
                        else {
                            var dem = parseInt($("span.chat-message-counter").html() == "" ? "0" : $("span.chat-message-counter").html()) + 1;
                            $('span.chat-message-counter').html(dem);
                        }
                    }
                    else if (dtoMess.Fr_UserRole == "HT") {
                        $('.chat-message-content').append('<div class="col-md-12"><em style="float:left">' + dtoMess.Message + '</em></div>');
                        if (!$('#live-chat').hasClass("d-none")) {
                            var height = $('.chat-message-content')[0].scrollHeight;
                            $('.chat-message-content').scrollTop(height);
                        }
                        else {
                            var dem = parseInt($("span.chat-message-counter").html() == "" ? "0" : $("span.chat-message-counter").html()) + 1;
                            $('span.chat-message-counter').html(dem);
                        }
                    }
                    else {
                        $('.chat-message-content').append('<div class="col-md-12"><div class="chat-message-text pull-right"><p>' + dtoMess.Message + '</p></div></div>');
                        var height = $('.chat-message-content')[0].scrollHeight;
                        $('.chat-message-content').scrollTop(height);
                    }
                }

                chatHub.client.sendErorr = function (title) {
                    Swal.fire({
                        title: title,
                        icon: 'error',
                        showCancelButton: false,
                        cancelButtonText: 'No',
                        confirmButtonText: 'OK'
                    })
                    .then(function (willDelete) {
                        if (willDelete.value) {
                            window.location.reload();
                        }
                    });
                }

                chatHub.client.onAllCSOffline = function (isOffline) {
                    $('#cs-online').addClass("d-none");
                    $('#cs-offline').removeClass("d-none");
                }
            }

        </script>*@
